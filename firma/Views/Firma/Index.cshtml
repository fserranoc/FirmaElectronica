
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @using (Html.BeginForm("UploadFile", "Firma",FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
       @Html.AntiForgeryToken()

        <div class="row">
            <input type="file" id="fileInput" name="fileInput"/>
        </div>
        <div class="row">
            <input type="submit" value="Firmar" class="btn btn-default" />
        </div>
    }
</body>
</html>
